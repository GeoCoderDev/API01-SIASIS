{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npx prisma generate",
        "nodeVersion": "18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ]
}